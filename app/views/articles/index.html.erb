<% if @articles.any? %>
<h1> All Articles </h1>

<ul id="articles">
<% @articles.each do |article| %>
	
 <%= link_to article.title, article_path(article), class: 'article_title' %>

<%end %>
</ul>
<% else %>
<p> No articles have been written yet </p>
<% end %>
<%= link_to 'Create New Article', new_article_path, class: "new_article" %>